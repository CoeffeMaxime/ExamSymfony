{% extends 'front/base.html.twig' %}


{% block title %}

{% endblock %}

{% block body %}


{% form_theme form 'bootstrap_5_layout.html.twig' %}

<div class="mx-auto col-8">
    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.image) }}
    {{ form_row(form.description) }}
    {{ form_row(form.producedYear) }}
    {{ form_row(form.mileage) }}
    {{ form_row(form.price) }}
    {{ form_row(form.model) }}
    <button class="btn btn-primary">
Envoyer
    </button>
    {{ form_end(form) }}
</div>
{% endblock %}